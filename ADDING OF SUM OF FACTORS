#include <stdio.h>

int main()
{
  int a,b,sum=0,h=0,count=0;
  scanf("%d",&a);
  printf("sum of factors");
  for(int i=1;i<a;i++)
  {
      if(a%i==0)
      { printf(" %d+",i);
          sum=sum+i;
          count++;
      }
  } printf(" = %d\n",sum);

 { for(int j=sum;j>=sum;j--)
  {
      b=sum%10;
      h=h+b;
   
     
      sum = sum/10;
      if (sum==0)
      break;
  } printf("\n");
  printf("Addition of sum of factors = %d",h); }
 
  

